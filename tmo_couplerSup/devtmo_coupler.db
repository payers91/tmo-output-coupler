#
# Simple protocol file for basic serial commands with the TMO output coupler
#
#  P
#  	EPICS prefix
#  DEV
#  	ASYN port

record(stringin, "$(P):NAME")
{
    field(DESC, "Chiller name")
    field(SCAN, "Passive")
}


#Trim command to move the potentiometers
record(bo, "$(P):TRIM")
{
    field(DESC, "Adjusts desired trimpot")
    field(DTYP, "stream")
    field(INP,  "@devtmo_coupler.proto set_trim(channel, time, speed) $(DEV)")
    field(SCAN, "Passive")
}

record(bo, "$(P):SWITCH")
{
    field(DESC, "Opens/Closes output switch")
    field(DTYP, "stream")
    field(INP, "@devtmo_coupler.proto set_switch(channel, value) $(DEV)")
    field(SCAN, "Passive")
}
